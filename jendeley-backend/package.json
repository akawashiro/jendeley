  },
  "homepage": "https://github.com/akawashiro/jendeley/jendeley-backend#readme",
  "devDependencies": {
    "@types/base-64": "^1.0.0",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "prettier": "2.8.0"
  },
  "dependencies": {
    "base-64": "^1.0.0",
    "body-parser": "^1.20.1",
