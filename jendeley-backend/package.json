    "fast-xml-parser": "^4.0.12",
    "got": "^12.5.3",
    "jest": "^29.3.1",
    "pdf-parse": "^1.1.1",
    "pino": "^8.7.0",
    "pino-pretty": "^9.1.1",
    "sanitize-html": "^2.7.3",
    "xml2js": "^0.4.23",
    "node-isbn": "^1.6.1"
  }
